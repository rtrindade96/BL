<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.RelRevest3Activity"
    xmlns:tools="http://schemas.android.com/tools"
    android:paddingLeft="16dp"
    android:paddingRight="16dp">


    <TextView
        android:id="@+id/OBSERVAÇÕES"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="OBSERVAÇÕES:"
        android:textSize="15dp"
        />

    <EditText
        android:id="@+id/editOVSERVAÇÕES"
        android:layout_width="350dp"
        android:layout_height="200dp"
        android:layout_below="@+id/OBSERVAÇÕES"
        android:layout_marginTop="5dp"
        android:background="@drawable/fundo_input_text"
        />





    <Button
        android:id="@+id/btconcluir"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:background="@drawable/fundo_bot"
        android:text="CONCLUIR"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textColor="#FFFFFF"
        android:layout_marginTop="20dp"
        />
</RelativeLayout>